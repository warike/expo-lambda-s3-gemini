{
  "name": "lambda",
  "version": "1.0.0",
  "description": "Warike - Mastra workflow for AWS Lambda",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "DOTENV_QUIET=1 jest --watchAll",
    "test:eval": "DOTENV_QUIET=1 vitest run src/__evals__/*scenario.spec.ts",
    "test:ci": "DOTENV_QUIET=1 jest --ci --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "devDependencies": {
    "@langwatch/scenario": "^0.3.0",
    "@types/aws-lambda": "latest",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^3.0.70",
    "@ai-sdk/google": "^2.0.46",
    "@aws-sdk/credential-providers": "^3.948.0",
    "@clerk/backend": "^2.27.0",
    "@mastra/ai-sdk": "1.0.0-beta.8",
    "@mastra/core": "1.0.0-beta.11",
    "@mastra/mongodb": "1.0.0-beta.6",
    "@mastra/observability": "1.0.0-beta.4",
    "@mastra/otel-exporter": "1.0.0-beta.5",
    "@mastra/rag": "2.0.0-beta.3",
    "@mastra/s3vectors": "1.0.0-beta.2",
    "ai": "^5.0.112",
    "aws-lambda": "^1.0.7",
    "dotenv": "^17.2.3",
    "zod": "^4.1.13"
  }
}